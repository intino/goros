def type(main)
    package $package.$module+lowerCase.box;

    public class Main {

        public static void main(String$[$] args) {
            $boxName+firstUpperCase~Box box = new $boxName+firstUpperCase~Box(args);
            box.start();
            Runtime.getRuntime().addShutdownHook(new Thread(box::stop));
        }

    }
end